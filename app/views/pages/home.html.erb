<h1>Pages - home</h1>
<% if current_user %>
    <h2><%= current_user.email %></h2>
    <h2><%= current_user.full_name %></h2>
    <img src=<%= current_user.avatar_url%>>
<% else %>
    <%= link_to "Login", new_user_session_path %>
    <%= link_to "Create Account", new_user_registration_path %>
<% end %>
